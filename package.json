{
	"name": "react-ifc-viewer",
	"version": "0.7.8",
	"description": "an IFC viewer for React",
	"author": "Antonio Colagreco",
	"license": "MIT",
	"type": "module",
	"files": [
		"dist",
		"LICENSE",
		"package.json",
		"README.md"
	],
	"types": "./dist/index.d.ts",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./css": "./dist/index.css"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/antoniocolagreco/react-ifc-viewer"
	},
	"scripts": {
		"dev": "storybook dev -p 6006",
		"build:lib": "tsc && vite build",
		"build:storybook": "storybook build",
		"typecheck": "tsc --noEmit",
		"lint": "tsc --noEmit && eslint ./src --ext .ts,.tsx",
		"lint:fix": "eslint ./src --ext .ts,.tsx --fix",
		"validate": "pnpm run typecheck && pnpm run lint",
		"format": "prettier . --write --ignore-unknown",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@storybook/addon-docs": "9.1.8",
		"@storybook/builder-vite": "^9.1.8",
		"@storybook/react": "^9.1.8",
		"@storybook/react-vite": "^9.1.8",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/eslint-plugin-jsx-a11y": "^6.10.0",
		"@types/node": "^24.5.2",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"@types/three": "^0.180.0",
		"@typescript-eslint/parser": "^8.44.1",
		"@vitejs/plugin-react-swc": "^4.1.0",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-jest-dom": "^5.5.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.22",
		"eslint-plugin-storybook": "^9.1.8",
		"eslint-plugin-unicorn": "^61.0.2",
		"globals": "^16.4.0",
		"jsdom": "^27.0.0",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"rollup": "^4.52.2",
		"rollup-preserve-directives": "^1.1.3",
		"storybook": "^9.1.8",
		"three": "^0.180.0",
		"typescript": "~5.9.2",
		"typescript-eslint": "^8.44.1",
		"vite": "^7.1.7",
		"vite-plugin-css-injected-by-js": "^3.5.2",
		"vite-plugin-dts": "^4.5.4",
		"vitest": "^3.2.4",
		"web-ifc": "^0.0.71"
	},
	"peerDependencies": {
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"three": "^0.175.0",
		"web-ifc": "^0.0.66"
	},
	"packageManager": "pnpm@9.14.4+sha512.c8180b3fbe4e4bca02c94234717896b5529740a6cbadf19fa78254270403ea2f27d4e1d46a08a0f56c89b63dc8ebfd3ee53326da720273794e6200fcf0d184ab",
	"dependencies": {
		"clsx": "^2.1.1"
	}
}
